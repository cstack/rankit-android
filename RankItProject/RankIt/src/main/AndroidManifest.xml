<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.connorstack.rankit"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.connorstack.rankit.PickOptionsActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.connorstack.rankit.PickFactorsActivity" />
        <activity
            android:name="com.connorstack.rankit.RankActivity"
            android:label="@string/title_activity_rank" >
        </activity>
        <activity
            android:name="com.connorstack.rankit.ResultsActivity"
            android:label="@string/title_activity_results" >
        </activity>
    </application>

</manifest>
